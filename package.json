{
  "name": "video-gallery-page",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0"
  },
  "scripts": {
    "start": "npm run server:dev",
    "server:dev": "webpack-dev-server --inline --hot --history-api-fallback --config build/webpack.base.config.js",
    "build:dev": "npm run clear && webpack --config build/webpack.base.config.js --progress --display-error-details",
    "build:prod": "npm run clear && webpack --config build/webpack.prod.config.js --progress --display-error-details",
    "clear": "rimraf dist/"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "jquery": "^3.2.1",
    "vue": "^2.3.3",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bootstrap": "^4.0.0-beta.2",
    "css-loader": "^0.28.2",
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^3.2.0",
    "eslint-plugin-import": "^2.2.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.3.1",
    "node-sass": "^4.5.3",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "vue-hot-reload-api": "^2.1.0",
    "vue-loader": "^13.0.4",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-node-externals": "^1.6.0"
  }
}
